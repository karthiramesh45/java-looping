import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int orginal=sc.nextInt();
		int n=orginal;
		int reversed=0;
		while(n!=0){
		    int digit=n%10;
		    reversed=reversed*10+digit;
		    n=n/10;
		 }
		 if(orginal==reversed){
		     System.out.println("palindrome");
		 }
		 else{
		     System.out.println("not a pali");
		 }
	}
}
