import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a=n*n;
		int sum=0;
		while (a > 0) {
            sum +=a % 10;
            a/= 10;
        }
        if(sum==n){
            System.out.println(n+":is neon");
        }
        else{
            System.out.println(n+":is not neon");

	}
}
}
