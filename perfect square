import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int sq = (int) Math.sqrt(n);

        if (sq * sq == n)
            System.out.println("Perfect square");
        else
            System.out.println("Not a perfect square");
    }
}
